	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=1132EF3D9C24C94691507D75B614B31C/Text/@EntryValue">internal class WeirdEnumCasingConvention : IMemberMapConvention&#xD;
{&#xD;
	public string Name { get { return "Weird Casing"; } }&#xD;
&#xD;
	public void Apply(BsonMemberMap memberMap)&#xD;
	{&#xD;
		if (memberMap.MemberType.IsEnum)&#xD;
		{&#xD;
			memberMap.SetSerializer(new WeirdEnumCasingSerializer());&#xD;
		}&#xD;
	}&#xD;
}</s:String>
